\n                <button class=\"input-group-text green\">\n                    <i class=\"fas fa-search text-grey\"\n                    aria-hidden=\"true\">\n                    </i>\n                </button>\n            </div>\n        </div>\n    </form>\n    </div>\n</template>\n\n<script>\nexport default {\n    data () {\n        return {\n            query: '',\n            page: 1,\n        }\n    },\n    methods: {\n        search () {\n            let URL = `${BASEURL}search/movie?api_key=${APIKEY}&language=es-MX&query=${this.query}&page=${this.page}`\n\n            fetch(URL)\n                .then(res => res.json())\n                .then(data  => {\n                    this.$emit('input', data)\n                })\n        },\n        setPage (page) {\n            this.page = page\n            this.search()\n        }, \n        resetSearch () {\n            this.query = ''\n            this.page = 1\n            this.$emit('input', {})\n        }\n    },\n}\n</script>\n\n<style>\n\n</style>"]}]}